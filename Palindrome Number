class Solution {
    public boolean isPalindrome(int x) {
        int t=x;
        int r,s=0;
        if(x>=0){
        while(x!=0){
          r=x%10;
          s=(s*10)+r;
          x=x/10;
        }
        if(t==s)
        {
            return true;
        }
        }
        return false;
    }
}
